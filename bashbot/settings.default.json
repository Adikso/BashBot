{
  "prefixes": [
    "$",
    ".bash"
  ],
  "short_tag": "$[*]",
  "terminal": {
    "refresh": 1,
    "init": [
      "export TERM=xterm",
      "clear"
    ],
    "shell_path": "/bin/bash",
    "su_path": "/bin/su"
  },
  "token": "",
  "redirect_tag": ">.bash",
  "delete_typed": "false",
  "show_cursor": "false",
  "terminal_template": "`| TTY #%s | %s | %s`\n```%s```",
  "presence_template": "@%s help",
  "custom_shortcuts": {
    "iambashbot": "echo BashBot is the best\n"
  },
  "user": {
    "login": "",
    "password": ""
  },
  "firstrun": true
}